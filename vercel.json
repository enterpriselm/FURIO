{
    "version": 2,
    "builds": [
      {
        "src": "api.py",
        "use": "@vercel/python"
      },
      {
        "src": "contact.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      { "src": "/api/.*", "dest": "api.py" },
      { "src": "/contact/.*", "dest": "contact.py" }
    ]
  }
  