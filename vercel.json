{
  "version": 2,
  "builds": [
    {
      "src": "api/api.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/contact.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/.*", "dest": "api/api.py" },
    { "src": "/contact/.*", "dest": "api/contact.py" },
    { "src": "/", "dest": "public/index.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
